<%= form_for(@event) do |f| %>
	<% if @event.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@event.errors.count, "error") %> prohibited
      this event from being saved:</h2>
    <ul>
    <% @event.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
	<%= f.label :title %>
	<%= f.text_field :title %><br />
	<%= f.label :lat %>
	<%= f.text_field :lat %><br />
	<%= f.label :lng %>
	<%= f.text_field :lng %><br />
	<%= f.label :street %>
	<%= f.text_field :street %><br />
	<%= f.label :city %>
	<%= f.text_field :city %><br />
	<%= f.label :state %>
	<%= f.text_field :state %><br />
	<%= f.label :zip %>
	<%= f.text_field :zip %><br />
	<%= f.label :starting %>
	<%= f.text_field :starting %><br />
	<%= f.label :duration %>
	<%= f.text_field :duration %><br />
	<%= f.label :capacity %>
	<%= f.text_field :capacity %><br />
	<%= f.label :description %>
	<%= f.text_area :description %><br />
	<%= f.label "Organizer" %>
	<%= f.select :user_id, 
								User.all.collect {|user| [user.username, user.id]},
								{prompt: "choose organizer"}
	 %><br />
	
	<%= f.submit %>
<% end %>

